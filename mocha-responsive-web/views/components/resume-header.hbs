{{#resume.basics}}
<header class="resume-header clearfix">
    <div class="profile-header">
        <h1 class="display-block" style="font-weight: 300;">{{name}}</h1>
        <h2 class="display-block" style="font-weight: 700; padding-top: 10px;">{{label}}</h2>
    </div>
    {{#if picture}}
    <div class="profile-pic">
        <img src="{{imgPathToBase64 picture}}" alt="profile-pic">
    </div>
    {{/if}}
</header>
{{/resume.basics}}
